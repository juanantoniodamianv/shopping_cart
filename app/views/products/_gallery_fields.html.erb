<fieldset>
	<div class="panel panel-default">
		<div class="panel-body">
			<div class="row">
				<div class="col-md-10">
					<div class="form-group ">
						<%= f.label :photo, "Imagen" %>
						<%= f.file_field :photo, class: "btn btn-info btn-xs"%>
						<%= f.hidden_field :photo_cache %>
				
					</div>

	
				</div>
				<div class="col-md-2">
					 <%= f.check_box :image_check %>
				</div>

			</div>
			          <%= f.hidden_field :_destroy %>
  <%= link_to  'Eliminar Imagen <i class="fa fa-times fa-1x" aria-hidden="true"></i>'.html_safe, '#', class: "remove_fields btn btn-danger" %>
		</div>
		
	</div>
</fieldset>